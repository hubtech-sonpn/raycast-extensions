{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "2qdb",
  "title": "2QDB",
  "description": "This is extension can quick query database",
  "icon": "command-icon.png",
  "author": "SonPham1602",
  "categories": [
    "Developer Tools",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "connect-database",
      "title": "Connect database",
      "description": "Connection to your database",
      "mode": "view"
    },
    {
      "name": "query",
      "title": "Query SQL",
      "description": "Execution your SQL",
      "mode": "view"
    },
    {
      "name": "database-list",
      "title": "Database List",
      "description": "View all database connection list",
      "mode": "view"
    },
    {
      "name": "history",
      "title": "History query",
      "description": "View history sql query",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.10.1",
    "mysql": "^2.18.1",
    "pg": "^8.11.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/mysql": "^2.15.25",
    "@types/node": "20.8.10",
    "@types/pg": "^8.10.9",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}